  "export": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test ava",
    "prepare": "node prepare.cjs || echo 'Skipping prepare'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonecorsi/lcov-total.git"
  },
  "author": "Simone Corsi <simonecorsi.dev@gmail.com>",
