{
  "name": "lcov-total",
  "version": "2.1.1",
  "description": "Parsing lcov.info and returning total of the summary intended to use inside CI to exit if threshold not reach",
  "bin": "bin/cmd.js",
  "export": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test ava",
    "prepare": "node prepare.mjs || echo 'Skipping prepare'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonecorsi/lcov-total.git"
  },
  "author": "Simone Corsi <simonecorsi.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonecorsi/lcov-total/issues"
  },
  "homepage": "https://github.com/simonecorsi/lcov-total#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "nodejs",
    "npm",
    "module",
    "javascript",
    "lcov",
    "parser",
    "total",
    "coverage",
    "total coverage",
    "test coverage",
    "nyc",
    "cli",
    "command",
    "util",
    "tdd",
    "testing",
    "test"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@saithodev/semantic-release-backmerge": "^3.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "ava": "^6.1.0",
    "lefthook": "^1.11.3",
    "markdown-toc": "^1.2.0",
    "semantic-release": "^24.2.3"
  },
  "volta": {
    "node": "20.13.1"
  }
}
