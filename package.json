  "bugs": {
    "url": "https://github.com/simonecorsi/lcov-total/issues"
  },
  "homepage": "https://github.com/simonecorsi/lcov-total#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "nodejs",
    "npm",
    "module",
    "javascript",
    "lcov",
    "parser",
    "total",
    "coverage",
