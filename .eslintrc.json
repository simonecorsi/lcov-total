{
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-new": "error",
    "no-process-exit": "off",
    "node/no-unsupported-features/es-syntax": "off"
  }
}
